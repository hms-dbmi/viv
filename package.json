{
  "name": "@hms-dbmi/viv",
  "version": "0.11.0",
  "main": "dist/viv.es.js",
  "module": "dist/viv.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "description": "High bit depth, high resolution, many channels, low overhead",
  "scripts": {
    "start": "vite",
    "build": "vite build --mode lib && tsc --emitDeclarationOnly",
    "build:avivator": "vite build",
    "preview": "vite preview",
    "prepare": "cd avivator && npm install",
    "test": "npm run test:layers_views && npm run test:loaders",
    "test:layers_views": "rollup -c rollup.config.test.js tests/layers_views/index.spec.js | browserify - | tape-run | tap-spec",
    "test:loaders": "rollup -c rollup.config.test.js tests/loaders/index.spec.js | node - | tap-spec",
    "test:prod": "npm run lint && npm run format && npm run test -- --no-cache",
    "lint": "eslint src/**/*.{js,jsx} avivator/src/**/*.{js,jsx} tests/**/*.js",
    "format": "prettier --check \"src/**/*.{js,ts,jsx}\" avivator/src/**/*.{js,jsx} tests/**/*.js",
    "format:write": "npm run format -- --write",
    "version": "./version.sh",
    "postversion": "git push && git push --tags",
    "docs": "mkdir -p dist_docs || rm -r dist_docs && mkdir -p dist_docs && documentation build src/**/*.jsx src/loaders/**/*.ts dist/viv.es.js -f html --document-exported --config documentation.yml --shallow -o dist_docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hms-dbmi/viv.git"
  },
  "keywords": [],
  "author": "Ilan Gold",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hms-dbmi/viv/issues"
  },
  "homepage": "https://github.com/hms-dbmi/viv#readme",
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "@deck.gl/core": "~8.6.0",
    "@deck.gl/geo-layers": "~8.6.0",
    "@deck.gl/layers": "~8.6.0",
    "@deck.gl/react": "~8.6.0",
    "@luma.gl/constants": "~8.5.3",
    "@luma.gl/core": "~8.5.3",
    "@luma.gl/shadertools": "~8.5.3",
    "@luma.gl/webgl": "~8.5.3"
  },
  "devDependencies": {
    "@deck.gl/test-utils": "^8.6.0",
    "@luma.gl/core": "^8.5.10",
    "@luma.gl/gltools": "^8.5.10",
    "@luma.gl/test-utils": "^8.5.10",
    "@probe.gl/test-utils": "^3.4.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@vitejs/plugin-react": "^1.0.1",
    "abortcontroller-polyfill": "^1.5.0",
    "browserify": "^17.0.0",
    "deck.gl": "^8.6.0",
    "documentation": "^13.1.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-app": "^6.2.2",
    "gl": "^4.4.1",
    "glsl-colormap": "^1.0.1",
    "prettier": "^2.2.1",
    "promise.prototype.finally": "^3.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-glslify": "^1.2.0",
    "serve-static": "^1.14.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "tape-catch": "^1.0.6",
    "tape-run": "^8.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.5.4"
  },
  "dependencies": {
    "@math.gl/culling": "^3.4.2",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^3.16.0",
    "geotiff": "https://github.com/ilan-gold/geotiff.js/archive/refs/tags/viv-0.0.2.tar.gz",
    "math.gl": "^3.3.0",
    "quickselect": "^2.0.0",
    "zarr": "^0.5.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
